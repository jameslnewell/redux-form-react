{
  "name": "redux-formo",
  "version": "3.0.0-preview.20",
  "description": "An alternate forms framework for Redux+React.",
  "keywords": [
    "redux-formo",
    "redux-form",
    "redux",
    "form",
    "react",
    "react-redux",
    "flux",
    "validate",
    "submit",
    "universaljs"
  ],
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:jameslnewell/redux-formo.git"
  },
  "dependencies": {
    "flux-standard-action": "^0.6.0",
    "invariant": "^2.1.1",
    "lodash.isequal": "^4.2.0"
  },
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.4.1",
    "deep-freeze-strict": "^1.1.1",
    "eslint": "^1.10.3",
    "eslint-config-jameslnewell": "^0.4.0",
    "mocha": "^2.3.3",
    "react": "^15.0.2",
    "react-redux": "^4.0.0",
    "redux-thunk": "^2.0.0",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0",
    "react-redux": "^4.0.0",
    "redux-thunk": "^1.0.0 || ^2.0.0"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "lint": "eslint ./index.js ./src/**/* --ext .js --ext .jsx",
    "test": "mocha --require ./dist/_.test.js ./dist/**/*.test.js ./dist/**/**/*.test.js",
    "test.watch": "npm run test -- --watch",
    "build": "babel --source-maps --extensions .js,.jsx --out-dir ./dist ./src",
    "watch": "npm run build -- --watch",
    "prepublish": "npm run clean && npm run build"
  },
  "license": "MIT"
}
